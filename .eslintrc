{
  "extends": "egg",
  "rules": {
    "array-bracket-spacing": [2, "never"],
    "vue/no-parsing-error": [2, {
      "x-invalid-end-tag": false
    }],
    "space-before-function-paren": [
      2, {
        "anonymous": "always",
        "named": "never",
        "asyncArrow": "never"
      }
    ]
  }
}